<h1>Suma salarial Session</h1>

<a asp-action="SessionSalarios" asp-controller="Empleados">
    Volver a almacenar salarios
</a>

@if (Context.Session.GetString("SUMASALARIAL") == null)
{
    <h1 style="color:red">No hay datos en Session</h1>
}
else
{
    <h1>
        Suma salarial: @Context.Session.GetString("SUMASALARIAL")
    </h1>
}