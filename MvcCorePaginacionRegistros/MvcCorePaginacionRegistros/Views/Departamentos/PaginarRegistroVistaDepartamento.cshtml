@model VistaDepartamento

@{
    ViewData["Title"] = "PaginarRegistroVistaDepartamento";
}

<h1>PaginarRegistroVistaDepartamento</h1>
<h2>Registro @Model.Posicion de @ViewData["ULTIMO"] </h2>

<div>
    <ul class="list-group list-group-horizontal">
        <li class="list-group-item list-group-item-warning">
            <a asp-controller="Departamentos"
               asp-action="PaginarRegistroVistaDepartamento"
               asp-route-posicion="1">
                Primero
            </a>
        </li>
        <li class="list-group-item list-group-item-warning">
            <a asp-controller="Departamentos"
               asp-action="PaginarRegistroVistaDepartamento"
               asp-route-posicion="@ViewData["ANTERIOR"]">
                Anterior
            </a>
        </li>
        <li class="list-group-item list-group-item-warning">
            <a asp-controller="Departamentos"
               asp-action="PaginarRegistroVistaDepartamento"
               asp-route-posicion="@ViewData["SIGUIENTE"]">
                Siguiente
            </a>
        </li>
        <li class="list-group-item list-group-item-warning">
            <a asp-controller="Departamentos"
               asp-action="PaginarRegistroVistaDepartamento"
               asp-route-posicion="@ViewData["ULTIMO"]">
                Ultimo
            </a>
        </li>
    </ul>
</div>

<div>
    <h4>VistaDepartamento</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Numero
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdDepartamento)
        </dd>
        <dt class = "col-sm-2">
            Nombre
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
        <dt class = "col-sm-2">
            Localidad
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Localidad)
        </dd>
        <dt class = "col-sm-2">
            Posicion
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Posicion)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new { /* id = Model.PrimaryKey */ }) |
    <a asp-action="Index">Back to List</a>
</div>
